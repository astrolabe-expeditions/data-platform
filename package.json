{
  "name": "data-platform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "format": "biome format . --write",
    "format:check": "biome format .",
    "lint": "biome lint . --write",
    "lint:check": "biome lint .",
    "preview": "vite preview",
    "seed:sync": "npx @snaplet/seed sync",
    "seed:generate": "npx --yes tsx supabase/seeds/seed.ts > supabase/seeds/default.sql",
    "type:generate": "supabase gen types typescript --local > src/shared/types/supabase.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/lab": "^6.0.0-beta.14",
    "@mui/material": "^6.1.7",
    "@mui/x-data-grid": "^7.23.5",
    "@refinedev/core": "^5.0.8",
    "@refinedev/mui": "^7.0.1",
    "@refinedev/react-hook-form": "^5.0.3",
    "@refinedev/react-router": "^2.0.3",
    "@refinedev/supabase": "^6.0.1",
    "@supabase/supabase-js": "^2.90.0",
    "bcryptjs": "^3.0.3",
    "dayjs": "^1.11.19",
    "i18next": "^25.7.4",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.70.0",
    "react-i18next": "^16.5.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@snaplet/copycat": "^6.0.0",
    "@snaplet/seed": "^0.98.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "postgres": "^3.4.8",
    "supabase": "^2.72.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "@snaplet/seed": {
    "config": "supabase/seeds/seed.config.ts"
  },
  "packageManager": "yarn@4.12.0"
}
